<Window x:Class="Vko46Autotalli.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vko46Autotalli"
        mc:Ignorable="d"
        Title="Villen autotalli" Height="350" Width="525">
    <Grid>
        <!-- ulompi stackpanel, elementit päällekäin pinossa-->
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button x:Name="btnHaeAutot" Content="Hae autot" Margin="5" Click="btnHaeAutot_Click">
                </Button>
                <Button x:Name="btnHaeAudit" Content="Hae Audit" Margin="5" Click="btnHaeAudit_Click">
                        
                    
                </Button>
            </StackPanel>
            <DataGrid x:Name="imgAutot" SelectionChanged="imgAutot_SelectionChanged">
                
            </DataGrid>
            <Image x:Name="imgAuto">
                
            </Image>
        </StackPanel>
    </Grid>
</Window>
